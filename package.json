{
  "name": "project-uno",
  "private": false,
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"tsx watch src/index.ts\"",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "tsx src/index.ts",
    "lint": "eslint .",
    "preview": "vite preview",
    "dc:build": "npm run build && docker compose build",
    "dc:up": "docker compose up -d",
    "dc:down": "docker compose down --remove-orphans && rm -rf dist",
    "dc:restart": "docker compose restart",
    "dc:logs": "docker compose logs -f",
    "dc:logs:app": "docker compose logs -f app",
    "dc:logs:db": "docker compose logs -f db",
    "dc:ps": "docker compose ps",
    "dc:clean": "docker compose down -v --remove-orphans && rm -rf dist",
    "dc:rebuild": "npm run build && docker compose down && docker compose build --no-cache && docker compose up -d",
    "dc:shell": "docker compose exec app sh",
    "dc:db:shell": "docker compose exec db psql -U megas -d monozao_db",
    "dcm:build": "docker compose -f docker-compose.mono.yml build",
    "dcm:up": "docker compose -f docker-compose.mono.yml up -d --remove-orphans",
    "dcm:down": "docker compose -f docker-compose.mono.yml down --remove-orphans && rm -rf dist",
    "dcm:restart": "docker compose -f docker-compose.mono.yml restart",
    "dcm:logs": "docker compose -f docker-compose.mono.yml logs -f",
    "dcm:logs:app": "docker compose -f docker-compose.mono.yml logs -f app",
    "dcm:ps": "docker compose -f docker-compose.mono.yml ps",
    "dcm:clean": "docker compose -f docker-compose.mono.yml down -v --remove-orphans && rm -rf dist",
    "dcm:rebuild": "npm run build && docker compose -f docker-compose.mono.yml down && docker compose -f docker-compose.mono.yml build --no-cache && docker compose -f docker-compose.mono.yml up -d --remove-orphans && rm -rf dist",
    "dcm:shell": "docker compose -f docker-compose.mono.yml exec app sh"
  },
